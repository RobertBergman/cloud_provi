# AWS Dual-Stack VPN Configuration
# Copy this to terraform.tfvars and fill in the router IPs from terraform-onprem-sim output

region      = "us-east-1"
environment = "prod"

# These values come from terraform-onprem-sim output
# Run: cd ../terraform-onprem-sim && terraform output
router_1_public_ip = "REPLACE_WITH_ROUTER_1_IP"  # From terraform-onprem-sim
router_2_public_ip = "REPLACE_WITH_ROUTER_2_IP"  # From terraform-onprem-sim

onprem_bgp_asn = 65001

test_instance_type = "t3.micro"
